{
    "description": "hiyoko api",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "unlicense",
    "authors": [
        {
            "name": "OpenAPI",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "vyuldashev/laravel-openapi": "^1.12",
        "tymon/jwt-auth": "^2.0",
        "laravel/sanctum": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-ide-helper": "^2.14",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
